{
  "name": "support-migrants-api",
  "version": "2.0.0",
  "description": "RESTful API to connect migrants with community organizations and institutions",
  "main": "src/main.js",
  "type": "module",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "dev": "NODE_ENV=development nodemon src/main.js",
    "start": "NODE_ENV=production node src/main.js",
    "test": "NODE_ENV=test mocha tests/**/*.test.js --exit",
    "test:coverage": "NODE_ENV=test c8 mocha tests/**/*.test.js --exit",
    "db:create:dev": "NODE_ENV=development npx sequelize-cli db:create",
    "db:drop:dev": "NODE_ENV=development npx sequelize-cli db:drop",
    "db:migrate:dev": "NODE_ENV=development npx sequelize-cli db:migrate",
    "db:seed:dev": "NODE_ENV=development npx sequelize-cli db:seed:all --seeders-path src/seeders/dev",
    "db:reset:dev": "NODE_ENV=development npm run db:drop:dev && npm run db:create:dev && npm run db:migrate:dev && npm run db:seed:dev",
    "db:undo:all:dev": "NODE_ENV=development npx sequelize-cli db:migrate:undo:all",
    "db:migrate:prod": "NODE_ENV=production npx sequelize-cli db:migrate",
    "db:seed:prod": "NODE_ENV=production npx sequelize-cli db:seed:all --seeders-path src/seeders/dev",
    "db:undo:all:prod": "NODE_ENV=production npx sequelize-cli db:migrate:undo:all",
    "db:setup": "npm run db:migrate:prod && npm run db:seed:prod",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Projeto-Migrantes/support-migrants-api.git"
  },
  "keywords": [
    "API",
    "REST",
    "Node.js",
    "PostgreSQL",
    "migrantes",
    "Supabase"
  ],
  "author": "Alisson Oliveira",
  "contributors": [
    {
      "name": "Alisson Oliveira",
      "url": "https://github.com/Alisson-Oliver"
    }
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Projeto-Migrantes/support-migrants-api/issues"
  },
  "homepage": "https://github.com/Projeto-Migrantes/support-migrants-api#readme",
  "dependencies": {
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "chai": "^5.1.1",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-rate-limit": "^7.4.1",
    "helmet": "^8.1.0",
    "joi": "^17.13.3",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "mocha": "^10.7.3",
    "morgan": "^1.10.0",
    "pg": "^8.13.1",
    "sequelize": "^6.37.4",
    "sequelize-cli": "^6.6.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "c8": "^9.1.0",
    "eslint": "^9.26.0",
    "globals": "^16.1.0",
    "nodemon": "^3.1.10",
    "sinon": "^17.0.1",
    "supertest": "^6.3.4"
  }
}
